package kr.co.sbaek76.ex9_classExtends;

class Calculator{
	double left, right;
	
	//public Calculator() {};
	
	public Calculator(int left, int right) {
		this.left = left;
		this.right = right;
	}
	
	public double add(){
		return (this.left + this.right);
	}
	
	public double sub() {
		return (this.left - this.right);
	}
	
	public double mul() {
		return (this.left * this.right);
	}
	
	public double div() {
		return (this.left / this.right);
	}
}

class Calculator2 extends Calculator {
	public Calculator2(int left, int right) {
		super(left, right);
	}
	
	public double avg() {
		return (div()/2);
	}
}

public class ex9_classExtends {
	public static void main(String[] args) {
		Calculator2 c1 = new Calculator2(192,282); 
		System.out.println(c1.mul());
		System.out.println(192./282.);
	}
}
