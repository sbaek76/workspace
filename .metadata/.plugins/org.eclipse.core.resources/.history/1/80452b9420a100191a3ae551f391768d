package kr.co.sbaek76.ex9_classExtends;

class Calculator{
	double left, right;
	
	//public Calculator() {};
	
	public Calculator(int left, int right) {
		this.left = left;
		this.right = right;
	}
	
	public double add(){
		return (this.left + this.right);
	}
	
	public double subtract() {
		return (this.left - this.right);
	}
	
	public double multiply() {
		return (this.left * this.right);
	}
	
	public double divide() {
		return (this.left / this.right);
	}
}

class Calculator2 extends Calculator {
	public Calculator2(int left, int right) {
		super(left, right);
	}
	
	public double avg() {
		return (this.divide()/2);
	}
}

public class ex9_classExtends {
	public static void main(String[] args) {
		Calculator2 c1 = new Calculator2(2,3); 
		System.out.println(c1.avg());
	}
}
